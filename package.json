{
  "name": "uniswap-v4-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/Uniswap/v4-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "run-s codegen && graph build",
    "build:docker": "docker build -t matchstick .",
    "buildonly": "graph build",
    "build:sepolia": "node scripts/generate-subgraph.js sepolia && run-s codegen && graph build --network sepolia",
    "build:mainnet": "node scripts/generate-subgraph.js mainnet && run-s codegen && graph build --network mainnet",
    "deploy:sepolia": "source .env && node scripts/generate-subgraph.js sepolia && run-s codegen && graph deploy uniswap-v-4 --node https://api.studio.thegraph.com/deploy/ --access-token $THEGRAPH_DEPLOY_KEY $THEGRAPH_SLUG_KEY_MAINNET_KEY_SEPOLIA --network sepolia",
    "deploy:mainnet": "source .env && node scripts/generate-subgraph.js mainnet && run-s codegen && graph deploy uniswap-v-4 --node https://api.studio.thegraph.com/deploy/ --access-token $THEGRAPH_DEPLOY_KEY $THEGRAPH_SLUG_KEY_MAINNET_KEY_MAINNET --network mainnet",
    "deploy:alchemy": "graph deploy uniswap-v-4 --node https://subgraphs.alchemy.com/api/subgraphs/deploy --ipfs https://ipfs.satsuma.xyz",
    "deploy:thegraph": "graph deploy uniswap-v-4",
    "deploy:hosted": "graph deploy uniswap-v-4 --node https://api.thegraph.com/deploy/",
    "deploy:retry:mainnet": "node scripts/deploy-with-retry.js mainnet",
    "deploy:retry:sepolia": "node scripts/deploy-with-retry.js sepolia",
    "auth:thegraph": "graph auth",
    "auth:hosted": "graph auth --node https://api.thegraph.com/deploy/",
    "create:hosted": "graph create --node https://api.thegraph.com/deploy/",
    "codegen": "graph codegen --output-dir src/types/",
    "test": "graph test -d",
    "test:no-tty": "docker run -i --rm --mount type=bind,source=$INIT_CWD,target=/matchstick matchstick",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.97.1",
    "@graphprotocol/graph-ts": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@uniswap/eslint-config": "^1.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^6.1.0",
    "matchstick-as": "^0.6.0",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "husky": "8.0.3",
    "lint-staged": "^15.2.7"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
